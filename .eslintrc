{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"arrow-parens":"off",
		"linebreak-style":"off",
		"comma-dangle":"off",
		"indent":"off",
		"no-tabs":"off",
		"eol-last":"off",
		"new-cap":"off",
		"no-undef":"warn",
		"no-invalid-this":"warn"

	},
	"extends": ["eslint:recommended", "google"]
}